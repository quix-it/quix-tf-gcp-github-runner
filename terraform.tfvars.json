{
  "runner": {   
    "type" : "e2-standard-2",
    "taint_labels" : true,
    "preemptible": true,
    "network": "runners",
    "image": "debian-runner",
    "os_label": "linux",
    "additional_labels": ["gcp"]
  },
  "scaling": {
    "idle_count" : 0,
    "idle_schedule" : "* 8-18 * * 1-5",
    "up_max" : 8,
    "up_rate" : 1,
    "down_rate" : 4,
    "down_schedule" : "*/5 * * * *",
    "grace_period_seconds": 120
  },
  "triggers": {
    "healthcheck_schedule" : "*/30 * * * *",
    "renew_schedule" : "0 2 * * *"
  }
}
